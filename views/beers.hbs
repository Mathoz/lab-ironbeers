<section id="beerPage">
    {{#each beers}}
        <div>
            <img src="{{this.image_url}}" alt="Beer image">
            <article>
                <h3>{{this.name}}</h3>
                <h4>{{this.tagline}}</h4>
                <p>{{this.description}}</p>
            </article>
        </div>
    {{/each}}
</section>
